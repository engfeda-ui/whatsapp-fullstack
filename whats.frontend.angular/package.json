{
    "name": "apollo-ng",
    "version": "19.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:prod": "ng serve --configuration production",
        "build": "ng build",
        "build:prod": "ng build --configuration production",
        "build:stats": "ng build --configuration production --stats-json",
        "watch": "ng build --watch --configuration development",
        "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
        "format:check": "prettier --check \"**/*.{js,mjs,ts,mts,d.ts,html}\"",
        "lint": "ng lint",
        "test": "ng test",
        "test:ci": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessCI",
        "test:coverage": "ng test --code-coverage",
        "analyze": "webpack-bundle-analyzer dist/stats.json",
        "docker:build": "docker build -t whats-frontend .",
        "docker:run": "docker run -p 80:80 whats-frontend",
        "docker:compose": "docker-compose up -d",
        "docker:down": "docker-compose down"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "19.2.16",
        "@angular/common": "19.2.16",
        "@angular/compiler": "19.2.16",
        "@angular/core": "19.2.16",
        "@angular/forms": "19.2.16",
        "@angular/platform-browser": "19.2.16",
        "@angular/platform-browser-dynamic": "19.2.16",
        "@angular/router": "19.2.16",
        "@primeng/themes": "^19.0.2",
        "chart.js": "4.4.2",
        "crypto-js": "^4.2.0",
        "jwt-decode": "^4.0.0",
        "primeicons": "^7.0.0",
        "primeng": "^19.0.8",
        "quill": "^2.0.3",
        "rxjs": "~7.8.0",
        "tailwindcss-primeui": "^0.5.1",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "19.2.16",
        "@angular-eslint/builder": "^20.5.0",
        "@angular-eslint/eslint-plugin": "^20.5.0",
        "@angular-eslint/eslint-plugin-template": "^20.5.0",
        "@angular-eslint/schematics": "^20.5.0",
        "@angular-eslint/template-parser": "^20.5.0",
        "@angular/cli": "19.2.16",
        "@angular/compiler-cli": "19.2.16",
        "@types/crypto-js": "^4.2.1",
        "@types/jasmine": "~5.1.0",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "jasmine-core": "~5.4.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.4.49",
        "prettier": "^3.0.0",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.6.2"
    }
}
